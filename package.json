{
  "name": "@paalan/tailwind-ui",
  "version": "2.0.3",
  "description": "This is a collection of React components and hooks that used tailwindcss as CSS framework.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "npm run storybook",
    "start": "npm run dev",
    "dev:watch": "concurrently \"npm:tsup:watch\" \"npm:tailwind:watch\"",
    "build:package": "npm run clean && npm run tsup && npm run tailwind -- --minify",
    "build": "npm run type-check && npm run lint && npm run build:package",
    "lint": "eslint ./src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --cache",
    "lint:fix": "npm run lint -- --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,md,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,md,mdx}\" --cache",
    "tsup": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" tsup",
    "tsup:watch": "tsup --watch",
    "tailwind": "npx tailwindcss -i ./src/globals.css  -o dist/styles.css",
    "tailwind:watch": "npm run tailwind -- --watch",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "npm run test -- --watch",
    "storybook": "storybook dev -p 6006",
    "storybook:docs": "storybook dev -p 6007 --docs",
    "storybook:build": "cross-env NODE_OPTIONS=\"--max-old-space-size=7096\" storybook build",
    "storybook:build:docs": "npm run storybook:build -- --docs",
    "storybook:test": "test-storybook",
    "storybook:test:watch": "npm run storybook:test -- --watch",
    "generate:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist",
    "prepare": "husky install",
    "clean": "rimraf dist",
    "validate:package-exports": "node ./scripts/validate-package-exports.js",
    "preview": "npm run storybook:build && serve storybook-static",
    "preview:docs": "npm run storybook:build:docs && serve storybook-static",
    "commit": "cz",
    "validate": "npm run build && npm run storybook:build && npm run test",
    "release": "release-it",
    "release:version": "npm run release -- --only-version"
  },
  "dependencies": {
